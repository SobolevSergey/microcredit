<?xml version="1.0"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 	<bean id="actionProcessor" class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName" value="java:module/ActionProcessorBean!ru.simplgroupp.interfaces.ActionProcessorBeanLocal"></property>
	</bean>
	
	<bean id="workflow" class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName" value="java:module/WorkflowBean!ru.simplgroupp.interfaces.WorkflowBeanLocal"></property>
	</bean>
</beans>